{
  "name": "js-hackday",
  "version": "1.0.0",
  "description": "JS hackday",
  "scripts": {
    "dev:server": "ENVIRONMENT=dev nodemon --experimental-json-modules -r dotenv/config src/server/index.js dotenv_config_path=./.local.env",
    "dev:client": "react-scripts start",
    "build": "react-scripts build",
    "start": "node --experimental-json-modules src/server/index.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "engines": {
    "node": "14.x"
  },
  "proxy": "http://localhost:4000",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markios/js-hackday.git"
  },
  "type": "module",
  "author": "Mark Kellett",
  "license": "ISC",
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/node-logger": "^6.4.9",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.4.9",
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.7"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@wellyshen/use-web-animations": "^0.9.1",
    "boom": "^7.3.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "socket.io": "^4.0.1",
    "socket.io-client": "^4.0.1",
    "uuid": "^8.3.2",
    "web-vitals": "^1.0.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
